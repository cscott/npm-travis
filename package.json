{
  "name": "npm-travis",
  "version": "1.0.1-git",
  "description": "Trigger travis jobs from npm",
  "main": "lib/index.js",
  "bin": {
    "npm-travis": "bin/npm-travis.js"
  },
  "scripts": {
    "test": "npm run lint-no-0.8",
    "lint": "npm run jshint && npm run jscs",
    "lint-no-0.8": "node -e 'process.exit(/v0[.][0-8][.]/.test(process.version) ? 0 : 1)' || npm run lint",
    "jshint": "jshint .",
    "jscs": "jscs .",
    "jscs-fix": "jscs --fix .",
    "travis": "bin/npm-travis.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cscott/npm-travis.git"
  },
  "keywords": [
    "travis",
    "npm"
  ],
  "author": "C. Scott Ananian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cscott/npm-travis/issues"
  },
  "dependencies": {
    "babybird": "^0.0.1",
    "commander": "^2.9.0",
    "core-js": "^2.3.0",
    "prfun": "^2.1.4",
    "pusher-client": "^1.1.0",
    "readable-stream": "^1.0.33",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "jscs": "^2.11.0",
    "jshint": "^2.9.2"
  }
}
